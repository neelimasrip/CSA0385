#include <stdio.h>

int main() {
    // Define an array of integers
    int arr[] = {10, 20, 30, 40, 50};
    int size = sizeof(arr) / sizeof(arr[0]);  // Calculate the size of the array

    // Print elements of the array
    printf("Elements of the array: ");
    for(int i = 0; i < size; ++i) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
